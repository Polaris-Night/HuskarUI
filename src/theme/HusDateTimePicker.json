{
    "__init__": {
        "__import__": ["HusInput"]
    },
    "__style__": {
        "fontFamily": "@fontPrimaryFamily",
        "fontSize": "@fontPrimarySize",
        "colorWeekText": "@colorTextBase",
        "colorIcon": "@colorTextTertiary",
        "colorIconHover": "@colorTextSecondary",
        "colorIconDisabled": "@colorTextQuaternary",
        "colorDayText": "@colorTextBase",
        "colorDayTextNone": "@colorTextQuaternary",
        "colorDayItemBgCurrent": "@colorPrimary",
        "colorDayItemBgHover": "$alpha(@colorPrimary, 0.6)",
        "colorDayItemBg": "transparent",
        "colorDayBgCurrent": "@colorPrimary",
        "colorDayBgHover": "$onBackground(@colorFill, @colorBgBase)",
        "colorDayBg": "$alpha(@colorBgBase, 0)",
        "colorDayBorderToday": "@colorPrimaryBorderActive",
        "colorPageIconHover": "@colorTextPrimary",
        "colorPageIcon": "@colorTextTertiary",
        "colorPageTextHover": "@colorPrimary",
        "colorPageText": "@colorTextBase",
        "colorSplitLine": "$alpha(@colorTextBase, 0.1)",
        "colorPopupText": "@colorTextBase",
        "colorPopupBg": "@colorBgBase",
        "colorPopupBgDark": "#222",
        "colorButtonBgHover": "$alpha(@colorTextBase, 0.05)",
        "colorButtonBgActive": "@colorPrimaryBase-1",
        "radiusBg": "@radiusPrimary",
        "radiusItemBg": "@radiusPrimarySM",
        "radiusPopupBg": "@radiusPrimary"
    }
}
